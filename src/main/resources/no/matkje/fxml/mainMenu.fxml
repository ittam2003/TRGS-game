<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="533.0" prefWidth="824.0" style="-fx-padding: 20; -fx-background-color: rgb(71, 62, 55);" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.matkje.gui.controller.MainMenuController">
   <center>
      <VBox alignment="CENTER" maxWidth="300.0" prefHeight="138.0" style="-fx-background-color: rgb(48, 43, 38); -fx-background-radius: 20; -fx-padding: 20;" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" style="-fx-spacing: 30;">
               <children>
                  <TextField fx:id="textField" />
                  <Button mnemonicParsing="false" onAction="#joinGame" prefHeight="25.0" prefWidth="51.0" text="Join" textFill="WHITE">
                     <font>
                        <Font name="Lato" size="12.0" />
                     </font></Button>
               </children>
            </HBox>
            <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="233.0" text="Host" textFill="WHITE">
               <font>
                  <Font name="Lato" size="12.0" />
               </font></Button>
         </children>
      </VBox>
   </center>
   <top>
      <ImageView fx:id="logo" fitHeight="231.0" fitWidth="720.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../media/title.png" />
         </image>
      </ImageView>
   </top>
</BorderPane>
